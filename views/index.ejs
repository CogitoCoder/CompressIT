  <!-- Main Content of the Web Page -->
  <main style="margin-bottom: 150px;">
    <div class="container">
      <h1 style="font-size:xx-large; font-weight: bold; letter-spacing: .1em; margin-bottom: 0; transform: translateX(1%);">
        CHOOSE FILES
      </h1>
      <%= typeof msg != 'undefined' ? msg : '' %>
      <form action="/" method="POST" enctype="multipart/form-data">
        <div class="file-field input-field" style="margin-top: 0;">
          <div class="btn" id="add">
            <span style="font-size: 20px;"><strong>+</strong></span>
            <input name="files" type="file" multiple required>
          </div>
          <br><br><br><br>
          <div class="file-path-wrapper" style="margin-top: 1em;">
            <input class="file-path validate" type="text" placeholder="Your Input File/Files Goes Here" style="font-weight: bolder;">
          </div>
        </div>
        <button id="compress" type="submit" class="btn">compress</button>
      </form>
    </div>
  </main>

